  <div class="container">
    <div class="row">
      <div class="col-md-6 col-md-offset-3">
        <h2>Welcome <span class="member-name"></span>!!!!</h2>
        <h2>Let's adds some bookmarks!!</h2>
      </div>
    </div>

     <div class="row">
      <div class="col-md-6 col-md-offset-3">
         {{#each bookmark}}
     
    
     <div class="col-md-6 col-md-offset-3" >
       <p>{{dataValues.title}}</p>
       <p><a target="_blank" href="https://{{dataValues.url}}"> {{dataValues.url}} </a></p>
      <button class="delete-bookmark btn btn-danger" data-id="{{dataValues.id}}">DELETE!</button>
    </div>
   
    
    {{else}}
     
      <p>No Bookmarks Saved yet.</p>
    
    {{/each}}
      </div>
    </div>
    
    
    
     

    
 
<form class="create-form">
      <div class="form-group">
        <div>
          <label for="bookmark">Bookmark Title:</label>
          <input type="text" id="title" name="name" />
        </div>
        <div>
          <label for="bookmark">Bookmark URL:</label>
          <input type="text" id="url" name="name" />
        </div>
        <div>
          <label for="bookmark">Bookmark Image:</label>
          <input type="text" id="image" name="name" />
        </div>
      </div>

      
      <button class="btn btn-success" id ="submitBtn"type="submit">Save your Bookmark</button>
    </form>
    </div>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
  <script type="text/javascript" src="js/addBookmark.js"></script>